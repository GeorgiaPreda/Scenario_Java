void update() {
  if ( overCircle(circleX, circleY, circleSize) ) {
    circleOver = true;
  }else {
    circleOver = false;
  }
}

void mousePressed() {
  if (circleOver) {
    background(27);
    magnifier++;
  }
  if(rectangleOver){
  background(27);
  offsetX=int(centerW);
  offsetY=int(centerH);
  }
}


  boolean overCircle(int x, int y, int diameter) {
  float disX = x - mouseX;
  float disY = y - mouseY;
  if (sqrt(sq(disX) + sq(disY)) < diameter/2 ) {
    return true;
  } else {
    return false;
  }
}

  
