void drawRoom(){
  //Finding out the margins of the room
    x1=xyRoomCoords[0][0];
    y1=xyRoomCoords[0][1];
    x2=xyRoomCoords[1][0];
    y2=xyRoomCoords[1][1];
    maxY=max(y1,y2);
    minY=min(y1,y2);
    maxX=max(x1,x2);
    minX=min(x1,x2);
    for(i=2;i<xyRoomCoords.length;i++){
        x1=x2;
        y1=y2;
        x2=xyRoomCoords[i][0];
        y2=xyRoomCoords[i][1];
        if(maxY<y1) maxY=y1;
        if(minY>y1) minY=y1;
        if(maxX<x1) maxX=x1;
        if(minX>x1) minX=x1;
        if(maxY<y2) maxY=y2;
        if(minY>y2) minY=y2;
        if(maxX<x2) maxX=x2;
        if(minX>x2) minX=x2;
    }
    
    //Finding the center of the new drawing
    diffX=maxX-minX;
    diffY=maxY-minY;
    centerWfig=mywidth/2+minX*magnifier-3+((abs(minX)+abs(maxX))*magnifier+6)/2;
    centerHfig=myheight/2+minY*magnifier-3+((abs(minY)+abs(maxY))*magnifier+6)/2;
    stroke(255);
    point(centerWfig,centerHfig);

    //Offset the figure center to the canvas center
    if(centerWfig<centerW) centerW = mywidth/2+(mywidth/2-centerWfig);
    else centerW = mywidth/2-(centerWfig-mywidth/2);
    if(centerHfig<centerH) centerH = myheight/2+(myheight/2-centerHfig);
    else centerH = myheight/2-(centerHfig-myheight/2);

  //ACTUAL DRAWING OF THE ROOM
