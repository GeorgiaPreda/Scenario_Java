    x1=xyRoomCoords[0][0];
    y1=xyRoomCoords[0][1];
    x2=xyRoomCoords[1][0];
    y2=xyRoomCoords[1][1];
    line(centerW+x1*magnifier, centerH+y1*magnifier, centerW+x2*magnifier, centerH+y2*magnifier);
